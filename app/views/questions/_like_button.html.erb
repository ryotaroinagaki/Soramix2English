<% if current_user.like?(question) %>
  <%= link_to question_like_path(question_id: question.id, id: current_user.likes.find_by(question_id: question.id)), data: { turbo_method: :delete } do %>
    <h1>unLike</h1>
  <% end %>
<% else %>
  <%= link_to question_likes_path(question_id: question.id), data: { turbo_method: :post } do %>
    <h1>Like</h1>
  <% end %>
<% end %>